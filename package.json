{
  "name": "next-boilerplate",
  "version": "0.0.1",
  "description": "Simple Next Boilerplate",
  "private": true,
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf .next",
    "svgr": "svgr --ext jsx -d src/assets/icons/components src/assets/icons/svg --icon --no-view-box",
    "dev": "(export CONFIG_ENV=dev NODE_ENV=production && npm run build && npm run start)",
    "preprod": "(export CONFIG_ENV=preprod NODE_ENV=production && npm run build && npm run start)",
    "prod": "(export CONFIG_ENV=prod NODE_ENV=production && npm run build && npm run start)",
    "hot": "(export NODE_ENV=development && npm run start)",
    "hot:dev": "(export CONFIG_ENV=dev && npm run hot)",
    "hot:preprod": "(export CONFIG_ENV=preprod && npm run hot)",
    "hot:prod": "(export CONFIG_ENV=prod && npm run hot)",
    "build": "next build",
    "start": "node server/index.js",
    "pre-commit": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint . --ext .js,.jsx",
    "lint:prettier": "prettier -c '**/*.{js,jsx}'",
    "lint:fix": "prettier --write '**/*.{js,jsx}' && eslint --fix .",
    "storybook": "start-storybook -p 6300",
    "compress-images": "node server/utils/compress-images.js",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run pre-commit"
    ]
  },
  "author": "cembakca",
  "license": "MIT",
  "dependencies": {
    "@storybook/addon-actions": "^5.2.1",
    "@weco/next-plugin-transpile-modules": "^2.2.1",
    "@zeit/next-sass": "^1.0.1",
    "axios": "^0.19.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-wildcard": "^5.0.0",
    "classnames": "^2.2.6",
    "config": "^3.2.2",
    "core-js": "^3.2.1",
    "express": "^4.17.1",
    "md5": "^2.3.0",
    "next": "^9.0.6",
    "next-redux-wrapper": "^3.0.0",
    "node-cache": "^5.1.0",
    "node-sass": "4.13.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-svg": "^10.0.21",
    "react-svg-loader": "^3.0.3",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "vanilla-lazyload": "^12.1.0",
    "webpack": "^4.40.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@storybook/addon-links": "^5.2.1",
    "@storybook/addons": "^5.2.1",
    "@storybook/react": "^5.2.1",
    "@svgr/cli": "^4.3.3",
    "@svgr/plugin-jsx": "^4.3.3",
    "autoconf": "^1.2.0",
    "compress-images": "1.5.8",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "3.0.5",
    "lint-staged": "^9.2.5",
    "mv": "^2.1.1",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.1"
  }
}
